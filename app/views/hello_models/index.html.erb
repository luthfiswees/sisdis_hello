<p id="notice"><%= notice %></p>

<h1>Listing Hello Models</h1>

<table>
  <thead>
    <tr>
      <th>Hellotext</th>
      <th>Uptime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hello_models.each do |hello_model| %>
      <tr>
        <td><%= hello_model.hellotext %></td>
        <td><%= hello_model.uptime %></td>
        <td><%= link_to 'Show', hello_model %></td>
        <td><%= link_to 'Edit', edit_hello_model_path(hello_model) %></td>
        <td><%= link_to 'Destroy', hello_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hello model', new_hello_model_path %>
